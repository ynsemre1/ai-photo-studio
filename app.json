{
  "expo": {
    "name": "AI Photo Studio",
    "slug": "gpt-creative-studio",
    "scheme": "aiphotostudio",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.studio.image.generator",
      "googleServicesFile": "./google-services.json"
    },
    "android": {
      "package": "com.fourstudios.generateai",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "com.android.vending.BILLING"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "642d1359-d6ab-40de-b074-8edd81183fb3"
      }
    },
    "owner": "xleria"
  }
}
