{
  "expo": {
    "name": "gpt-creative-studio",
    "slug": "gpt-creative-studio",
    "scheme": "gptstudio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.studio.image.generator",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.fourstudios.generateai",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "642d1359-d6ab-40de-b074-8edd81183fb3"
      }
    }
  }
}